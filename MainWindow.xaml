<Window x:Class="U5Quadratic.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:U5Quadratic"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Canvas x:Name="canvas">
        <TextBox x:Name="TextboxA" Canvas.Left="40" Canvas.Top="31" Width="19" Height="19"/>
        <TextBox x:Name="TextboxB" Canvas.Left="88" Canvas.Top="31" Width="19" Height="19" />
        <TextBox x:Name="TextboxC" Canvas.Left="64" Canvas.Top="31" Width="19" Height="19" />
        <Button x:Name="btnInput" Click="btnInput_Click" Content="Click" Canvas.Left="40" Canvas.Top="55" Height="21" Width="67" />
        <Label x:Name="lblOutput" Canvas.Left="40" Canvas.Top="81" Height="91" Width="112" />
        <TextBlock Canvas.Left="40" Canvas.Top="10" Width="19">a</TextBlock>
        <TextBlock Canvas.Left="64" Canvas.Top="10" Width="19">b</TextBlock>
        <TextBlock Canvas.Left="88" Canvas.Top="10" Width="19">c</TextBlock>
    </Canvas>
</Window>
